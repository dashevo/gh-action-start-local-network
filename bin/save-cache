#!/usr/bin/env node

const cache = require('@actions/cache');

async function main() {
  const path = process.argv[2];
  const key = process.argv[3];

  try {
    await cache.saveCache([path], key);
  } catch (e) {
    if (typeof e.message !== 'string' || !e.message.includes('Unable to reserve cache with key')) {
      throw e;
    }
  }
}

main().catch((e) => {
  console.error(e);
  process.exit(1);
});
